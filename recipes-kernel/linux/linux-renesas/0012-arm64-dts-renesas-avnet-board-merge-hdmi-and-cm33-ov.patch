From f0bade52cda37a2c119de4ab7f11996a2f591e6c Mon Sep 17 00:00:00 2001
From: Vu Dang <vu.dang.te@renesas.com>
Date: Mon, 19 Dec 2022 09:49:26 +0700
Subject: [PATCH 12/15] arm64: dts: renesas: avnet-board: merge hdmi and cm33
 overlays to rzboard dts

Signed-off-by: Vu Dang <vu.dang.te@renasas.com>
---
 arch/arm64/boot/dts/renesas/rzboard.dts | 111 ++++++++++++++++++++++++
 1 file changed, 111 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/rzboard.dts b/arch/arm64/boot/dts/renesas/rzboard.dts
index 8a53329a229b..c8a13fee19d5 100755
--- a/arch/arm64/boot/dts/renesas/rzboard.dts
+++ b/arch/arm64/boot/dts/renesas/rzboard.dts
@@ -11,6 +11,7 @@
 #include <dt-bindings/gpio/gpio.h>
 #include <dt-bindings/pinctrl/rzg2l-pinctrl.h>
 #include <dt-bindings/input/input.h>
+#include <dt-bindings/interrupt-controller/arm-gic.h>
 
 / {
 	model = "Avnet RzBoard (RZV2L Dual Cortex-A55)";
@@ -800,3 +801,113 @@ sd1_ctrl_uhs {
 		};
 	};
 };
+
+/* ================== Begin hdmi ================================*/
+&du {
+        dsi-lanes = <4>;
+        status = "okay";
+};
+
+
+&it6161 {
+        hdmi-output-1080p;
+        status = "okay";
+        ports {
+                #address-cells = <1>;
+                #size-cells = <0>;
+
+                port@0 {
+                        reg = <0>;
+                        it6161_in: endpoint@0 {
+                                remote-endpoint = <&dsi0_out>;
+                        };
+                };
+        };
+};
+
+&dsi0 {
+        status = "okay";
+
+        ports {
+                port@1 {
+                        dsi0_out: endpoint {
+                                remote-endpoint = <&it6161_in>;
+                                data-lanes = <1 2 3 4>;
+                        };
+                };
+        };
+};
+
+&hdmi_out {
+        status = "okay";
+};
+
+&ssi1 {
+        status = "okay";
+};
+
+/* ================== Begin cm33 ================================*/
+/* Disable scif22 and OSTM2 to be used in FreeRTOS on Cortex-M33 */
+
+/ {
+        compatible = "avnet,rzboard", "renesas,r9a07g054";
+        fragment@2 {
+                target-path = "/";
+                __overlay__ {
+                        #address-cells = <2>;
+                        #size-cells = <2>;
+
+                        vring-ctl0@43000000 {
+                                compatible = "vring_uio";
+                                reg = <0x0 0x43000000 0x0 0x100000>;
+                                no-map;
+                        };
+                        vring-ctl1@43100000 {
+                                compatible = "vring_uio";
+                                reg = <0x0 0x43100000 0x0 0x100000>;
+                                no-map;
+                        };
+
+                        vring-shm0@43200000 {
+                                compatible = "shm_uio";
+                                reg = <0x0 0x43200000 0x0 0x300000>;
+                                no-map;
+                        };
+                       vring-shm1@43500000 {
+                                compatible = "shm_uio";
+                                reg = <0x0 0x43500000 0x0 0x300000>;
+                                no-map;
+                        };
+
+                        rsctbl@42f00000 {
+                                compatible = "shm_uio";
+                                reg = <0x0 0x42f00000 0x0 0x1000>;
+                                no-map;
+                        };
+                        mhu-shm@42f01000 {
+                                compatible = "shm_uio";
+                                reg = <0x0 0x42f01000 0x0 0x1000>;
+                                no-map;
+                        };
+
+                        mbox-uio@10400000 {
+                                compatible = "mbox_uio";
+                                reg = <0x0 0x10400000 0x0 0x1000>;
+                                interrupt-parent = <&gic>;
+                                interrupts = <GIC_SPI 75 IRQ_TYPE_LEVEL_HIGH>;
+                                status = "okay";
+                        };
+                };
+        };
+};
+
+&scif2 {
+        status = "disabled";
+};
+
+&ostm2 {
+        status = "disabled";
+};
+
+
+
